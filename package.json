{
  "name": "swanky-dev-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./CLIENTSIDE/static",
    "checkYarn": "if brew ls --versions yarn > /dev/null; then echo 'yarn is installed'; else brew install yarn; fi ",
    "checkMongo": "if brew ls --versions mongodb > /dev/null; then echo 'mongodb is installed'; else brew install mongodb; fi ",
    "turnOnMongo": "brew services start mongodb",
    "build:webpack": "NODE_ENV=production webpack --config ./SERVERSIDE/webpack/webpack.config.prod.js",
    "build-production": "npm run clean && npm rebuild node-sass && npm run build:webpack",
    "start": "node ./SERVERSIDE/startup",
    "start-staging": "NODE_ENV=production HOST_MODE=staging node ./SERVERSIDE/startup",
    "start-production": "NODE_ENV=production node ./SERVERSIDE/startup",
    "checkDeps": "npm run checkMongo && npm run turnOnMongo && npm run checkYarn",
    "setup": "git pull origin master && npm run checkDeps && yarn install && npm run build-production && npm run start",
    "heroku-postbuild": "npm run build-production && ls ./CLIENTSIDE/static",
    "lint": "eslint"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "~1.18.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^1.3.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "gsap": "^1.20.3",
    "heroku-ssl-redirect": "^0.0.4",
    "hex-to-rgba": "^1.0.1",
    "jquery": "^3.2.1",
    "mobx": "^3.1.15",
    "mobx-react": "^4.2.1",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^4.10.4",
    "morgan": "~1.8.2",
    "node-sass": "^4.5.3",
    "on-build-webpack": "^0.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "query-string": "^5.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-json-tree": "^0.11.0",
    "react-transition-group": "^2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "serve-favicon": "~2.4.3",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.18.1",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "depcheck": "^0.6.8",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "react-hot-loader": "^1.3.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4"
  }
}
